{
    "manifest_version": 2,
    "name": "Random Number Generator",
    "description": "A random number generator with different features",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "popup.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}